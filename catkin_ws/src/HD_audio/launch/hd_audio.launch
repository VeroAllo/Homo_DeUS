<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="public_sim"    default="true"/>

  <!-- audio parameters -->
  <arg name="format"     default="mp3"/>

    <!-- https://wiki.ros.org/roslaunch/XML/node -->
  <node if="true" name="talk_interface" pkg="HD_audio" type="talkInterface.py" respawn="false" output="screen" launch-prefix="gnome-terminal --command"/>
</launch>